        "app": "ios.release"
      },
      "ios.release.17.2": {
        "device": "simulator.17.2",
        "app": "ios.release"
      },
      "android.debug": {
        "device": "emulator",
        "app": "android.debug"
      },
      "android.release": {
        "device": "emulator",
        "app": "android.release"
      }
    }
  },
