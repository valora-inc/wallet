  "scripts": {
    "prebuild": "expo prebuild --clean",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "env:mainnet": "APP_VARIANT=mainnet eas env:pull production --non-interactive",
    "env:mainnet-dev": "APP_VARIANT=mainnet-dev eas env:pull development --non-interactive",
    "env:mainnet-nightly": "APP_VARIANT=mainnet-nightly eas env:pull preview --non-interactive",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext=.tsx,.ts .",
    "format": "prettier --loglevel error --write .",
    "format:check": "prettier --check .",
    "knip": "knip",
    "reset": "yarn reset-modules && yarn reset-yarn",
    "reset-modules": "rm -rf node_modules/",
    "reset-yarn": "yarn cache clean",
    "check-licenses": "yarn licenses list --prod | grep '\\(─ GPL\\|─ (GPL-[1-9]\\.[0-9]\\+ OR GPL-[1-9]\\.[0-9]\\+)\\)' && echo 'Found GPL license(s). Use 'yarn licenses list --prod' to look up the offending package' && exit 1 || echo 'No GPL licenses found'",
    "sync-eas-node-version": "ts-node ./scripts/sync-eas-node-version.ts",
    "pre-deploy": "yarn version --no-git-tag-version",
    "generate-release-notes": "ts-node ./scripts/generate-release-notes.ts",
    "postinstall": "patch-package"
  },
